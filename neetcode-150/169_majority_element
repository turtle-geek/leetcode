#include <vector>

class Solution {
public:
    int majorityElement(std::vector<int>& nums) {
        /**
         * Time Complexity: O(n) for iterating through array
         * Space Complexity: O(1) for fixed number of variables
         */
        int count = 0;
        int res;
        for(int num : nums){
            if(count == 0){
                res = num;
            }

            if(num == res){
                count++;
            } else {
                count--;
            }
        }
        return res;
    }
};